{
  "name": "@handbook/babel-plugin",
  "version": "0.4.0",
  "description": "Babel plugin for handbook",
  "main": "lib/plugin.js",
  "typings": "lib/plugin.d.ts",
  "author": "Seoyeon Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-zeroconfig/handbook-babel-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/react-zeroconfig/handbook-babel-plugin/issues"
  },
  "homepage": "https://github.com/react-zeroconfig/handbook-babel-plugin",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "docs": "markdown-source-import \"{,!(node_modules)/**/}*.md\"",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "watch": "npm run clean && tsc --watch --project tsconfig.build.json",
    "test": "jest --colors",
    "coverage": "jest --colors --coverage",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,mjs,ts,tsx}\"",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "pack": "npm run prepublishOnly && npm pack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "markdown-source-import \"{,!(node_modules)/**/}*.md\" --git-add && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "prettier --write",
      "eslint",
      "jest --colors --findRelatedTests"
    ]
  },
  "prettier": "@lunit/prettier-config",
  "eslintConfig": {
    "extends": [
      "@lunit/eslint-config/without-react",
      "prettier",
      "prettier/@typescript-eslint"
    ]
  },
  "dependencies": {
    "@babel/traverse": "^7.8.6",
    "@babel/types": "^7.8.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@lunit/eslint-config": "^0.4.0",
    "@lunit/prettier-config": "^0.1.1",
    "@types/babel__traverse": "^7.0.9",
    "@types/eslint": "^6.1.8",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/prettier": "^1.19.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
